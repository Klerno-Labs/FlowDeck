{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/send-reminders",
      "schedule": "*/5 * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/studio/:path*",
      "destination": "https://ftc-flowdeck.sanity.studio/studio/:path*"
    }
  ]
}
